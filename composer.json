{
    "name": "nabu-3/core",
    "description": "Core package to deploy nabu-3 Framework in your server",
    "version": "3.0.12",
    "type": "project",
    "keywords": [ "nabu", "nabu-3", "core", "framework", "frameworks", "smarty" ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rafael GutiÃ©rrez",
            "email": "ragutimar@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/nabu-3/core/issues"
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "lib"
    },
    "require": {
        "php": ">=7.0",
        "nabu-3/composer-installer-plugin": ">=3.0",
        "nabu-3/provider-mysql-driver": ">=0.0",
        "nabu-3/provider-apache-httpd": ">=0.0",
        "nabu-3/sdk": ">=3.0"
    },
    "require-dev": {
        "php": ">=7.0",
        "nabu-3/composer-installer-plugin": "dev-master",
        "nabu-3/provider-mysql-driver": "dev-master",
        "nabu-3/provider-apache-httpd": "dev-master",
        "nabu-3/sdk": "dev-master"
    },
    "suggest": {
        "nabu-3/provider-apache-apcu": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nabu-3/core"
        }
    ],
    "bin": [
        "bin/nabu-install.sh",
        "bin/nabu-messaging-daemon.sh"
    ]
}
