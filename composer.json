{
    "name": "nabu-3/core",
    "description": "Core package to deploy nabu-3 Framework in your server",
    "version": "3.0.3",
    "type": "project",
    "keywords": [ "nabu", "nabu-3", "core", "framework", "frameworks", "smarty" ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rafael Guti√©rrez",
            "email": "ragutimar@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/nabu-3/core/issues"
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "lib"
    },
    "require": {
        "nabu-3/composer-installer-plugin": "*",
        "nabu-3/provider-mysql": "*",
        "nabu-3/provider-smarty": "*",
        "nabu-3/provider-apache-httpd": "*"
    },
    "suggest": {
        "nabu-3/provider-apache-apcu": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nabu-3/core"
        }
    ],
    "bin": [
        "bin/nabu-install.sh"
    ]
}
